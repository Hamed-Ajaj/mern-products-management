#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hamed-archy/workshop-projects/products-management/server/node_modules/.pnpm/strip-json-comments@1.0.4/node_modules/strip-json-comments/node_modules:/home/hamed-archy/workshop-projects/products-management/server/node_modules/.pnpm/strip-json-comments@1.0.4/node_modules:/home/hamed-archy/workshop-projects/products-management/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hamed-archy/workshop-projects/products-management/server/node_modules/.pnpm/strip-json-comments@1.0.4/node_modules/strip-json-comments/node_modules:/home/hamed-archy/workshop-projects/products-management/server/node_modules/.pnpm/strip-json-comments@1.0.4/node_modules:/home/hamed-archy/workshop-projects/products-management/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../strip-json-comments@1.0.4/node_modules/strip-json-comments/cli.js" "$@"
else
  exec node  "$basedir/../../../../../strip-json-comments@1.0.4/node_modules/strip-json-comments/cli.js" "$@"
fi
